{
  "name": "@mtgedh/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
      "blight:audit": "node --enable-source-maps --import tsx/esm scripts/blight-audit.ts",
    "intervening-if:audit": "node --enable-source-maps --import tsx/esm scripts/intervening-if-audit.ts",
    "intervening-if:backlog": "node --enable-source-maps --import tsx/esm scripts/intervening-if-backlog.ts",
    "intervening-if:categorize": "node --enable-source-maps --import tsx/esm scripts/intervening-if-categorize.ts",
    "dev": "tsx watch --watch-path=src src/index.ts",
    "dev:server:noreload": "npm run dev:noreload",
    "dev:noreload": "npm run start",
    "start": "node --enable-source-maps --import tsx/esm src/index.ts",
    "targeting:coverage": "node --enable-source-maps --import tsx/esm scripts/targeting-coverage.ts",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "npm run typecheck",
    "test:vitest": "vitest run",
    "test:vitest:watch": "vitest"
  },
  "dependencies": {
    "better-sqlite3": "^9.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.19.3",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
